
@{
    ViewData["Title"] = "Index";
  //  Layout = "~/Views/Shared/_Layout.cshtml";
      Layout = "~/Views/Shared/_Layout1.cshtml";
}

<h1>Index</h1>
<h1>Temp From Index</h1>

<h2>@ViewBag.Data1</h2>
<h2>@ViewData["Data2"]</h2>
<h3>@TempData["Data3"]</h3>

@{
    foreach(var item in (string[])ViewData["StrArr"])
    {
        <h2>@item</h2>
    }

    foreach (var item in (List<string>)ViewData["StrList"])
    {
        <h2>@item</h2>
    }
}




